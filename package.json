{
  "name": "wb-idp",
  "version": "1.2.1",
  "description": "WB IDP - Integrated Data Portal",
  "keywords": [
    "Oskari"
  ],
  "dependencies": {
    "oskari-frontend": "file:../oskari-frontend",
    "oskari-frontend-contrib": "file:../oskari-frontend-contrib"
  },
  "scripts": {
    "build": "node ./node_modules/oskari-frontend/node_modules/webpack/bin/webpack.js --config ./node_modules/oskari-frontend/webpack.config.js --mode production",
    "start": "node ./node_modules/oskari-frontend/node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./node_modules/oskari-frontend/webpack.config.js --hot --env.appdef=applications/wbidp",
    "test": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geoenvo/oskari-application-dps.git"
  },
  "private": true
}
